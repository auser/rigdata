app.config(['$routeProvider', ($routeProvider) ->

  $routeProvider
    .when '/data',
      templateUrl: "<%= asset_path '/templates/home.html' %>"
      controller: "HomeController"
    .when '/help',
      templateUrl: "<%= asset_path '/templates/help.html' %>"
      controller: "HelpController"
    .otherwise
      redirectTo: '/data'
      
  ])